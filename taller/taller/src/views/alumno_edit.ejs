<% include partials/_header %>
<div class="container mt-5">
	<div class="row">
		<div class="col-md-5">
			<div class="card-body">
			<%=console.log(data.id_alumno);%>
				<form action="/update/<%= data.id_alumno %>" method="post" accept-charset="utf-8">
					<div class= "form-group">
					<input type="text" name="nombre"  value="<%= data.nombre %>" class = "form-control">
				</div>
				<div class= "form-group">
					<input type="text" name="apellidoPaterno" value="<%= data.apellidoPaterno %>" class = "form-control">
				</div>
				<div class= "form-group">
					<input type="text" name="apellidoMaterno" value="<%= data.apellidoMaterno %>" class = "form-control">
				</div>
				<div class= "form-group">
					<input type="text" name="ci" value="<%= data.ci %>" class = "form-control">
				</div>
				<div class= "form-group">
					<input type="text" name="cu" value="<%= data.cu %>" class = "form-control">
				</div>
				<div class= "form-group">
					<input type="text" name="carrera" value="<%= data.carrera %>" class = "form-control">
					<div class= "form-group">
					<input type="hidden" name="updated_at" value=<%=new Date().toJSON().slice(0,10); %> class = "form-control">
				</div>
				<button type="submit" class="btn btn-primary">
				Actualizar
				</button>
				</form>
			</div>
			
		</div>
		
	</div>
	
</div>
<% include partials/_footer %>